                        <div class="pcoded-inner-navbar main-menu">

                            <div class="pcoded-navigatio-lavel">Assn Member</div>
                            <ul class="pcoded-item pcoded-left-item">
                                <li>
                                    <a href='@Url.Action("MemberManagement", "AssnMember")' class='pcoded-link'>
                                        <span class="pcoded-micon"><i class="icofont icofont-engineer"></i></span>
                                        <span class="pcoded-mtext">現有協會會員</span>
                                        <span class="pcoded-mcaret"></span>
                                    </a>
                                </li>
                                <li>
                                    <a href='@Url.Action("MemberDetailedInfo", "AssnMember")' class='pcoded-link'>
                                        <span class="pcoded-micon"><i class="icofont icofont-engineer"></i></span>
                                        <span class="pcoded-mtext">協會成員詳細資料</span>
                                        <span class="pcoded-mcaret"></span>
                                    </a>
                                </li>
                            </ul>

                        </div>
                    </nav>
                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">
                            <div class="main-body">
                                <div class="page-wrapper">
                                    <div class="page-header">
                                        <div class="page-header-title">
                                            <h4>@RenderSection("HeaderTitle", required: true)</h4>
                                        </div>
                                    </div>
                                    <div class="page-body">
                                        @RenderBody()
                                    </div>
                                </div>
                            </div>
                            <!--<div id="styleSelector">-->
                            <!--</div>-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
