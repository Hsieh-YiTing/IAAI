    <!-- Morris Chart Custom js -->
    <script type="text/javascript" src="~/Content/pages/dashboard/project-dashboard.js"></script>
    <!-- Custom js -->
    <script type="text/javascript" src="~/Scripts/js/script.js"></script>
    <script src="~/Scripts/js/pcoded.min.js"></script>
    <script src="~/Scripts/js/demo-12.js"></script>
